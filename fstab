/dev/mapper/vg_middlewaremagic-lv_root /                       ext4    defaults        1 1
UUID=30025f1b-aa57-4fd3-81c1-48f52ee06642 /boot                   ext4    defaults        1 2
/dev/mapper/vg_middlewaremagic-lv_swap swap                    swap    defaults        0 0
tmpfs                   /dev/shm                tmpfs   defaults        0 0
devpts                  /dev/pts                devpts  gid=5,mode=620  0 0
sysfs                   /sys                    sysfs   defaults        0 0
proc                    /proc                   proc    defaults        0 0
